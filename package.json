{
  "name": "unsplash-photos-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "unsplash-js": "^4.8.0"
  },
  "scripts": {
    "start": "parallelshell \"npm run sass:watch\" \"react-scripts start\" ",
    "watch": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass --output-style expanded --watch --source-map true -o ./src/styles/css/ ./src/styles/scss/",
    "sass:build": "node-sass --output-style expanded --source-map true -o ./src/styles/css/ ./src/styles/scss/",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "node-sass": "^4.9.0",
    "parallelshell": "^3.0.2"
  }
}
